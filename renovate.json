{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "regexManagers": [
        {
            "fileMatch": [".Dockerfile$"],
            "matchStrings": ["# renovate packageName=(?<depName>.*?)\n.*JDK_VERSION=(?<currentValue>.*?)\n"],
            "datasourceTemplate": "repology",
            "versioningTemplate": "loose"
        }
    ]
  }
  